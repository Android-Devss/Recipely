<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/screen_one"
        tools:context=".ui.onboarding.FirstScreenOnBoarding">


    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:id="@+id/skip"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/space24"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/skip"
        android:textColor="@color/white_fa"
        android:textSize="@dimen/text18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

        <com.google.android.material.button.MaterialButton
            android:layout_width="match_parent"
            android:id="@+id/next"
            android:layout_height="wrap_content"
            android:text="@string/next"
            android:layout_margin="@dimen/space24"
            app:cornerRadius="@dimen/space8"
            android:padding="@dimen/space12"
            android:textColor="@color/white_fa"
            android:backgroundTint="@color/secondary"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="VisualLintButtonSize" />


    </androidx.constraintlayout.widget.ConstraintLayout>